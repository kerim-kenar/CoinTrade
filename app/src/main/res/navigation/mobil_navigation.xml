<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobil_navigation"
    app:startDestination="@id/buySellFragment">


    <fragment
        android:id="@+id/marketsFragment"
        android:name="com.kerimkenar.cointrade.view.MarketsFragment"
        android:label="fragment_markets"
        tools:layout="@layout/fragment_markets" />
    <fragment
        android:id="@+id/assetsFragment"
        android:name="com.kerimkenar.cointrade.view.AssetsFragment"
        android:label="fragment_assets"
        tools:layout="@layout/fragment_assets" />
    <fragment
        android:id="@+id/buySellFragment"
        android:name="com.kerimkenar.cointrade.view.BuySellFragment"
        android:label="fragment_buy_sell"
        tools:layout="@layout/fragment_buy_sell" />

    <fragment
        android:id="@+id/depositCheckFragment"
        android:name="com.kerimkenar.cointrade.view.DepositCheckFragment"
        android:label="fragment_deposit_check"
        tools:layout="@layout/fragment_deposit_check" />

    <fragment
        android:id="@+id/accountFragment"
        android:name="com.kerimkenar.cointrade.view.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />

    <activity
        android:id="@+id/mainActivity"
        android:name="com.kerimkenar.cointrade.view.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />



</navigation>